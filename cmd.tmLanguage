			<array>
				<dict>
					<key>include</key>
					<string>#batch</string>
				</dict>
			</array>
		</dict>
		<key>cmd-redirect</key>
		<dict>
			<key>match</key>
			<string>&amp;&amp;|&amp;|\|\||\||&amp;&gt;|\d?&gt;&amp;\d?|\d?(&gt;&gt;|&gt;|&lt;)|\d?&lt;&amp;|\d?&lt;&gt;</string>
			<key>name</key>
			<string>markup.inserted.redirect.shell</string>
		</dict>
		<key>cmd-rem</key>
		<dict>
			<key>captures</key>
			<dict>
				<key>1</key>
				<dict>
					<key>name</key>
					<string>keyword.rem.cmd</string>
				</dict>
				<key>2</key>
				<dict>
					<key>name</key>
					<string>comment.rem.cmd</string>
				</dict>
			</dict>
			<key>match</key>
			<string>\b(?i)(rem)\b(?!\s*/\?)(.*?)(?&lt;!\^)(?=$|&amp;|\||&lt;|&gt;)</string>
		</dict>
		<key>cmd-rem-colons</key>
		<dict>
			<key>match</key>
			<string>::.*(?=$|&amp;|\||&lt;|&gt;)</string>
			<key>name</key>
			<string>comment.rem.cmd</string>
		</dict>
		<key>cmd-reserved</key>
		<dict>
			<key>match</key>
