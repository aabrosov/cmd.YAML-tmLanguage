			<key>match</key>
			<string>:\w+</string>
			<key>name</key>
			<string>constant.other.colon.cmd</string>
		</dict>
		<key>cmd-net</key>
		<dict>
			<key>match</key>
			<string>\b(?i)net(\.exe)?\s+(accounts|computer|config|continue|file|group|help|helpmsg|localgroup|pause|session|share|start|statistics|stop|time|use|user|view)\b</string>
			<key>name</key>
			<string>keyword.net.exe.cmd</string>
		</dict>
		<key>cmd-parenthesis</key>
		<dict>
			<key>begin</key>
			<string>\(</string>
			<key>captures</key>
			<dict>
				<key>0</key>
				<dict>
					<key>name</key>
					<string>markup.inserted.cmd</string>
				</dict>
			</dict>
			<key>end</key>
			<string>\)</string>
