		<key>cmd-com</key>
		<dict>
			<key>match</key>
			<string>\b(?i)(chcp|diskcomp|diskcopy|format|mode|more|tree)(\.com)?\b</string>
			<key>name</key>
			<string>keyword.external.com.cmd</string>
		</dict>
		<key>cmd-echo</key>
		<dict>
			<key>captures</key>
			<dict>
				<key>1</key>
				<dict>
					<key>name</key>
					<string>keyword.echo.cmd</string>
				</dict>
				<key>2</key>
				<dict>
					<key>name</key>
					<string>string.echo.cmd</string>
				</dict>
			</dict>
			<key>match</key>
			<string>\b(?i)(echo)\b(?!\s*/\?)(.*?)(?&lt;!\^)(?=$|&amp;|\||&lt;|&gt;)</string>
		</dict>
		<key>cmd-echo-on</key>
		<dict>
			<key>match</key>
			<string>\b(?i)echo\s+(on|off|/\?)\s*(?=$|&amp;|\||&lt;|&gt;)</string>
