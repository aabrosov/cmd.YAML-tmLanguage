			<key>captures</key>
			<dict>
				<key>1</key>
				<dict>
					<key>name</key>
					<string>keyword.echo.cmd</string>
				</dict>
				<key>2</key>
				<dict>
					<key>name</key>
					<string>string.echo.cmd</string>
				</dict>
			</dict>
			<key>match</key>
			<string>\b(?i)(title)\b(?!\s*/\?)(.*?)(?&lt;!\^)(?=$|&amp;|\||&lt;|&gt;)</string>
		</dict>
		<key>cmd-vars-args</key>
		<dict>
			<key>match</key>
			<string>%%?~?d?p?n?x?s?a?t?z?(\$PATH:)?[0-9a-zA-Z\*]</string>
			<key>name</key>
			<string>variable.parameter.argument.cmd</string>
		</dict>
		<key>cmd-vars-exclam</key>
		<dict>
			<key>match</key>
			<string>!.*?!</string>
			<key>name</key>
			<string>variable.parameter.exclamation.cmd</string>
		</dict>
		<key>cmd-vars-percent</key>
		<dict>
			<key>match</key>
			<string>%[\w$]+(:~-?\d+,-?\d+|:[^=%]+=[^=%]+)?%</string>
			<key>name</key>
			<string>variable.parameter.percent.cmd</string>
		</dict>
